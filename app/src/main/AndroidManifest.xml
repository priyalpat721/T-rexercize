<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.neu.madcourse.trexercize">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_trex"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_trex_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Trexercize">
        <activity
            android:name="edu.neu.madcourse.trexercize.ui.fragments.userAuth.SignInActivity"
            android:exported="false" />
        <activity
            android:name="edu.neu.madcourse.trexercize.ui.fragments.userAuth.SignUpActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.HomeActivity"
            android:exported="false">
        </activity>
    </application>

</manifest>